<resources>

    <!-- Base application theme. @color/colorAccent

    @color/colorPrimaryDark
    -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/weatherColor</item>
        <item name="colorPrimaryDark">@color/colorPrimary</item>
        <item name="colorAccent">@color/weatherColor</item>
    </style>
    <style name="white" parent="AlertDialog.AppCompat.Light">
        <item name="android:textColor">#FFFFFF</item>
        <item name="android:textSize">20dp</item>
    </style>
    <style name="wind" parent="white">
        <item name="android:textSize">23dp</item>
    </style>
    <style name="test" parent="AppTheme">
        <item name="colorAccent">#ff0000</item>
    </style>

</resources>
